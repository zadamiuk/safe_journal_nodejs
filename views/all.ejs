

  <div class="row mt-4 mb-5">
    <div class="col-md-3">
    <div class="card mb-4">
    <div class="card-body">
    <h4 class="card-title d-flex justify-content-between align-items-center">
      <form action="/notes/all" method="GET"><table>
          <thead>
            <tr>
            <th>Title</th>
            </tr>
            </thead>
          <tbody>
          <%Note.forEach( function( data){ %> 
            
            <tr>
              <td><%= data.title %></td>
              <td><a href="/notes/edit" class="fixed" data-q_id=data._id><i class="fas fa-edit"></i></a></td>
            </tr>
          
          <% }); %>
          </tbody>
          </table>
      </form>
    </h4>
    <form action="/notes/delete/{{_id}}?_method=DELETE" method="POST">
    <input type="hidden" name="_method" value="DELETE">
    <button class="btn btn-danger btn-block btn-sm" type="submit"> 
    Delete
    </button>
  </form>
    </div>   
    </div>   
  </div>  
